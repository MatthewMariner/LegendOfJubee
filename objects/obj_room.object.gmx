          <argument>
            <kind>1</kind>
            <string>///Draw room to surface

surface_set_target(room_surface);
draw_clear_alpha(c_black, 0);

//Looping through the 1's and 2's and 3's data array set , to know which tile to show on screen
tiles = string_to_array(room_data, " ");

//Loop through tile and draw bg title
for (var i = 0; i &lt; 176; i++) {
    var tile = tiles[i];
    var tile_size = 16;
    var xx, yy;
    
    xx = (i mod 16) * tile_size;
    yy = (i div 16) * tile_size;
    
    //what tile to grab and where to paint it
    switch(tile) {
        case "2": {
            draw_background_part(bg_overworld_green, 1 * tile_size, 0, tile_size, tile_size, xx , yy);
            break;
        }
        case "3": {
            draw_background_part(bg_overworld_green, 2 * tile_size, 0, tile_size, tile_size, xx, yy);
            break;
        }
    }     
}

surface_reset_target();
</string>
